{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Docusaurus v2",
  "docs": "https://v2.docusaurus.io/docs/typescript-support",
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["DOM"],
    "moduleResolution": "node",
    "noEmit": true,
    "types": ["node", "@docusaurus/module-type-aliases", "@docusaurus/theme-classic", "docusaurus-plugin-sass"],
    "baseUrl": ".",
    "resolveJsonModule": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@site/*": ["./*"],
      "@edim-editor/emoji": ["../emoji/src/index.ts"],
      "@edim-editor/heading": ["../heading/src/index.ts"],
      "@edim-editor/command": ["../command/src/index.ts"],
      "@edim-editor/hr": ["../hr/src/index.ts"],
      "@edim-editor/paragraph": ["../paragraph/src/index.ts"],
      "@edim-editor/flat-list": ["../flat-list/src/index.ts"],
      "@edim-editor/flat-task-list": ["../flat-task-list/src/index.ts"],
      "@edim-editor/tables": ["../tables/src/index.ts"],
      "@edim-editor/image": ["../image/src/index.ts"],
      "@edim-editor/link": ["../link/src/index.ts"],
      "@edim-editor/core": ["../core/src/index.ts"],
      "@edim-editor/codeblock": ["../codeblock/src/index.ts"],
      "@edim-editor/blockquote": ["../blockquote/src/index.ts"],
      "@edim-editor/bold": ["../bold/src/index.ts"],
      "@edim-editor/code": ["../code/src/index.ts"],
      "@edim-editor/font-family": ["../font-family/src/index.ts"],
      "@edim-editor/italic": ["../italic/src/index.ts"],
      "@edim-editor/strikethrough": ["../strikethrough/src/index.ts"],
      "@edim-editor/subscript": ["../subscript/src/index.ts"],
      "@edim-editor/superscript": ["../superscript/src/index.ts"],
      "@edim-editor/text-color": ["../text-color/src/index.ts"],
      "@edim-editor/underline": ["../underline/src/index.ts"],
      "@edim-editor/mention": ["../mention/src/index.ts"],
      "@edim-editor/ui": ["../ui/src/index.ts"],
      "@edim-editor/menubar": ["../menubar/src/index.ts"],
      "@edim-editor/preset": ["../preset/src/index.ts"]
    },
    "skipLibCheck": true
  }
}
