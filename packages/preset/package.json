{
  "name": "@edim-editor/preset",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "tsup --watch --tsconfig tsconfig.build.json",
    "clean:dist": "rimraf dist",
    "clean:dependency": "rimraf node_modules",
    "build": "npm run clean:dist && npm run build:ts && npm run build:style",
    "build:ts": "tsup --tsconfig tsconfig.build.json",
    "build:style": "sass styles:styles"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "style": "styles/preset.css",
  "types": "dist/index.d.ts",
  "private": true,
  "dependencies": {
    "prosemirror-commands": "*",
    "prosemirror-inputrules": "*",
    "prosemirror-keymap": "*",
    "prosemirror-model": "*",
    "prosemirror-state": "*",
    "@edim-editor/core": "*",
    "@edim-editor/blockquote": "*",
    "@edim-editor/codeblock": "*",
    "@edim-editor/command": "*",
    "@edim-editor/emoji": "*",
    "@edim-editor/flat-list": "*",
    "@edim-editor/flat-task-list": "*",
    "@edim-editor/heading": "*",
    "@edim-editor/hr": "*",
    "@edim-editor/image": "*",
    "@edim-editor/link": "*",
    "@edim-editor/mention": "*",
    "@edim-editor/paragraph": "*",
    "@edim-editor/bold": "*",
    "@edim-editor/code": "*",
    "@edim-editor/font-family": "*",
    "@edim-editor/italic": "*",
    "@edim-editor/strikethrough": "*",
    "@edim-editor/subscript": "*",
    "@edim-editor/superscript": "*",
    "@edim-editor/text-color": "*",
    "@edim-editor/underline": "*",
    "@edim-editor/tables": "*",
    "@edim-editor/ui": "*",
    "@edim-editor/menubar": "*"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2",
    "sass": "^1.69.5"
  }
}
